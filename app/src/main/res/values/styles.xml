<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.BunkPolice.Styled.TextView.Inter.Regular" parent="android:Widget.TextView">
        <item name="android:fontFamily">@font/inter_regular</item>
    </style>

    <style name="Theme.Tasky.Styled.Button.Inter.Regular" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/rounded_button</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/White</item>
    </style>

    <style name="Theme.Tasky.Styled.EditText.Inter.Regular" parent="Widget.AppCompat.EditText">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColorHint">@color/DarkGray</item>
        <item name="android:textColor">@color/Black</item>
    </style>


</resources>